{
  "manifest_version": 3,
  "name": "HelpMeOut",
  "description": "HelpMeOut",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "32": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
